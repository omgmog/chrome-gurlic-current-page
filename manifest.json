{
    "manifest_version": 2,
    "name": "Gurlic this page",
    "description": "This extension will let you share the current page on Gurlic.",
    "version": "1.0",
    "browser_action": {
        "default_icon": "icon.png"
    },
    "background": {
        "scripts": [
            "ext.js",
            "background.js"
        ]
    },
    "permissions": [
       "tabs"
    ],
    "content_security_policy": "script-src 'self'; object-src 'self'"
}
